{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "singularName": "place",
    "pluralName": "places",
    "displayName": "Place",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "place_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "place_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::place-category.place-category",
      "inversedBy": "places"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "location": {
      "type": "customField",
      "options": {
        "info": true
      },
      "customField": "plugin::strapi-location-picker.location-picker"
    }
  }
}
