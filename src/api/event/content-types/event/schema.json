{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "start_date": {
      "type": "datetime",
      "default": "2025-05-26T17:15:12.449Z"
    },
    "end_date": {
      "type": "datetime"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "event_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "clover_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "event_images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "place": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::place.place"
    }
  }
}
